{
    "name": "Custom Facebook",
    "manifest_version": 2,
    "version": "0.0.3",
    "icons": {
        "32": "images/thumbs/32x32.png",
        "48": "images/thumbs/48x48.png",
        "128": "images/thumbs/128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.facebook.com/*"],
            "css": ["styles/default.css"],
            "js": ["scripts/app.js"]
        }
    ],
    "web_accessible_resources": [
        "images/icons/favicon.ico"
    ]
}
